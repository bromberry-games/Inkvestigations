{
	"name": "mystery-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm:dev-vite\" \"npm:dev-stripe\"",
		"dev-vite": "vite dev",
		"dev-stripe": "stripe listen --forward-to http://localhost:5173/api/webhook",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen_types": "supabase gen types typescript --local > ./src/schema.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@poppanator/sveltekit-svg": "^4.1.3",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-cloudflare": "^2.3.2",
		"@sveltejs/kit": "^1.20.4",
		"@types/express": "^4.17.20",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"express": "^4.18.2",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"supabase": "^1.83.7",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vite-plugin-windicss": "^1.9.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@inqling/svelte-icons": "^3.4.3",
		"@popperjs/core": "^2.11.8",
		"@supabase/auth-helpers-sveltekit": "^0.10.2",
		"@supabase/auth-ui-shared": "^0.1.6",
		"@supabase/auth-ui-svelte": "^0.2.2",
		"@supabase/supabase-js": "^2.32.0",
		"classnames": "^2.3.2",
		"eventsource-parser": "^1.1.1",
		"flowbite": "^1.6.5",
		"flowbite-svelte": "^0.37.4",
		"flowbite-svelte-icons": "^0.3.6",
		"gpt3-tokenizer": "^1.1.5",
		"highlight.js": "^11.8.0",
		"lucide-svelte": "^0.241.0",
		"openai": "^3.3.0",
		"random-word-slugs": "^0.1.7",
		"snarkdown": "^2.0.0",
		"sse": "^0.0.8",
		"sse.js": "^0.6.1",
		"stripe": "^12.17.0",
		"svelte-legos": "^0.2.1",
		"svelte-local-storage-store": "^0.5.0",
		"tailwind-merge": "^1.14.0",
		"tailwindcss": "^3.3.2",
		"uuid": "^9.0.0"
	}
}
